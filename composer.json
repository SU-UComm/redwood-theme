{
  "name": "stanford/redwood",
  "description": "Stanford WordPress theme. Based on Timber starter theme.",
  "type":"wordpress-theme",
  "minimum-stability" : "stable",
  "authors": [
      {
          "name": "jbchrsty",
          "email": "jbchrsty@stanford.edu"
      }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "extra": {
  	"installer-paths": {
      "../../plugins/{$name}/": [
        "wpackagist-plugin/*",
        "type:wordpress-plugin"
      ]
  	}
	},
  "require": {
    "timber/timber": "2.0.0",
    "twig/twig": "3.9.3"
  },
  "autoload": {
    "psr-4": {
      "Stanford\\Redwood\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
